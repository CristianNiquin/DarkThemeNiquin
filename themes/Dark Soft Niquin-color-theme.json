{
	"name": "Dark Soft Niquin",
	"colors": {
		"editor.background": "#1A1B1C", //"#202020",
		"editor.foreground": "#dbdbdb",
		"activityBarBadge.background": "#2a4353",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#797979"//"#6A9955"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#EBA568"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"meta.preprocessor.numeric",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent",
				"variable.other.enummember",
		],
			"settings": {
				"foreground": "#86be9f"//"#b5cea8"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"entity.name.function.preprocessor",
				"keyword.operator.noexcept",
				"keyword",
				"keyword.type",
				"meta.diff.header",
				"meta.preprocessor",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded",
				"punctuation.support.type.property-name.begin.json",
				"punctuation.support.type.property-name.end.json",
				"support.type.property-name.json",
				"storage",
				"storage.modifier",
				"storage.type",
				"variable.language",
			],
			"settings": {
				"foreground": "#5898c2"
			}
		},
		{
			"name": "Keys",
			"scope": [
				"constant.language.null",
				"constant.language.boolean",
			],
			"settings": {
				"foreground": "#FC548E"//"#4EBEEF"//"#569CD6"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ce78b4"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly",
				"string.regexp",
			],
			"settings": {
				"foreground": "#ffa7d3"//"#78ce86"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#b581c7"//"#5898c2"//"#b782ca"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#FF9090"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#5898c2"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#b5cea8"
			}
		},



		// START HTML
		{
			"name": "Decorators HTML",
			"scope": [
				"punctuation.definition.tag.begin.html",	
				"punctuation.definition.tag.end.html",

				"meta.tag.structure.div.start.html",
				"meta.tag.structure.div.end.html",				
				"punctuation.separator.key-value.html",				
				// "text.aspnetcorerazor",

				"punctuation.definition.tag.xml",				
			],
			"settings": {
				"foreground": "#AAA"
			}
		},
		{
			"name": "Attributes HTML",
			"scope": [
				"entity.other.attribute-name.html",
				"entity.other.attribute-name",
				
				"keyword.control.cshtml.transition",
			],
			"settings": {
				"foreground": "#43AD98"
			}
		},
		{
			"name": "Atributo Quote HTML",
			"scope": [
				"punctuation.definition.string.begin.html",	
				"punctuation.definition.string.end.html",	
				// "meta.tag.structure.div.start.html",
				// "meta.tag.structure.div.end.html",

				"string.quoted.double.xml",
				"string.quoted.double.html",
				"meta.attribute.class.html",
				"meta.attribute.src.html",
				"meta.tag.object.img.void.html",

				"invalid.illegal.character-not-allowed-here.html",
			],
			"settings": {
				"foreground": "#E0DB9B",
			}
		},
		{
			"name": "Atributo Quote HTML",
			"scope": [	
				"entity.name.tag.html",
				"meta.tag.structure.div.start.html",
				"meta.tag.structure.div.end.html",
				"entity.name.tag.localname.xml",
			],
			"settings": {
				"foreground": "#FC548E"
			}
		},
		{
			"name": "Constant HTML",
			"scope": [
				"punctuation.definition.entity.html",	
				"constant.character.entity.named.nbsp.html",
			],
			"settings": {
				"foreground": "#b581c7"
			}
		},
		// END HTML



		// START JS
		{
			"name": "Metadata",
			"scope": [	
				"meta.brace.round.js",	
				"meta.brace.square.js",
				"meta.definition.variable.js",
				
				"punctuation.definition.block.js",	
				"punctuation.definition.parameters.begin.js",
				"punctuation.definition.parameters.end.js",
			],
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Key Reserved",
			"scope": [
				"storage.modifier.async.js",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Flow Control",
			"scope": [
				"keyword.control.conditional.js",
				"keyword.control.flow.js",
			],
			"settings": {
				"foreground": "#b581c7"
			}
		},
		{
			"name": "Types and Functions",
			"scope": [
				"storage.type.js",
				"storage.type.function.js",

				"constant.language.boolean.true.js",
				"constant.language.boolean.false.js",
			],
			"settings": {
				"foreground": "#5898c2"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter.js",
				"variable.other.object.js",	
				"variable.other.constant.js",
				"variable.other.readwrite.js",
			],
			"settings": {
				"foreground": "#a9d2ee"//"#a4d1ee"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"storage.type.function.arrow.js",
				"punctuation.accessor.js",	
				"punctuation.separator.key-value.js",	
			],
			"settings": {
				"foreground": "#ffa0a0"
			}
		},
		{
			"name": "String JS",
			"scope": [
				"string.quoted.single.js",
				"string.quoted.double.js",
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
			],
			"settings": {
				"foreground": "#ffa0cf"//"#ffa0cf"
			}
		},
		// END JS

	],
	
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#d4d4d4",
		"stringLiteral": "#ce9178",
		"customLiteral": "#D4D4D4",
		"numberLiteral": "#b5cea8",
	},
		
}
